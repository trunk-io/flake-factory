Install maven from ubuntu

sudo apt install maven -y

# install packags                                                                                           
sudo apt-get install \
libatk1.0-0 \
libatk-bridge2.0-0 \
libatspi2.0-0 \
libxcomposite1 \
libxdamage1 \
libxfixes3 \
libxrandr2 \
libgbm1 \
libxkbcommon0 \
libpango-1.0 \
libcairo2
                                                                                            
                                                                                               
                                                                                         
         

mvn compile
mvn test

